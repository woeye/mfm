{
  "name": "mfm",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build --turbopack",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev --turbopack",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start"
  },
  "dependencies": {
    "@mantine/core": "^8.3.3",
    "@mantine/hooks": "^8.3.3",
    "@payloadcms/db-mongodb": "~3.59.1",
    "@payloadcms/db-postgres": "~3.59.1",
    "@payloadcms/db-sqlite": "~3.59.1",
    "@payloadcms/live-preview-react": "~3.59.1",
    "@payloadcms/next": "~3.59.1",
    "@payloadcms/payload-cloud": "~3.59.1",
    "@payloadcms/plugin-seo": "~3.59.1",
    "@payloadcms/richtext-lexical": "~3.59.1",
    "@payloadcms/storage-s3": "^3.59.1",
    "@payloadcms/ui": "~3.59.1",
    "cross-env": "^10.1.0",
    "date-fns": "^4.1.0",
    "dotenv": "17.2.3",
    "graphql": "^16.11.0",
    "next": "^15.5.4",
    "payload": "~3.59.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0",
    "sharp": "^0.34.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@next/bundle-analyzer": "^15.5.4",
    "@next/eslint-plugin-next": "^15.5.4",
    "@stylistic/eslint-plugin": "^5.4.0",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/postcss": "^4.1.14",
    "@types/node": "^24.7.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "eslint": "^9.37.0",
    "eslint-config-next": "15.5.4",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.4.0",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "^1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "3.6.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.14",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.45.0"
  },
  "engines": {
    "node": ">=24.9.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ]
  }
}